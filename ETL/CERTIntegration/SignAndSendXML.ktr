    <definitions>
      <definition>
        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>import java.utils.*;
import java.lang.*;
import java.net.*;
import sun.security.util.DerInputStream;
import sun.security.util.DerValue;


import javax.xml.bind.DatatypeConverter;
import java.io.IOException;
import java.math.BigInteger;
import java.nio.file.*;
import java.nio.file.Paths;
import java.nio.*;
import java.security.*;
import java.security.spec.InvalidKeySpecException;
import java.security.spec.RSAPrivateCrtKeySpec;
import java.security.spec.X509EncodedKeySpec;

String FILENAMEField;	
String PRIVATE_KEY_PATHField;	
String RESPONSEField;	
String CERT_URLField;			
									

 public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {
        if (first) {
            FILENAMEField = getParameter("FILENAMEField");
            PRIVATE_KEY_PATHField = getParameter("PRIVATE_KEY_PATHField");
            RESPONSEField = getParameter("RESPONSEField");
            CERT_URLField = getParameter("CERT_URLField");
            first = false;
        }

        Object[] r = getRow();

        if (r == null) {
            setOutputDone();
            return false;
        }

        String FILENAME = get(Fields.In, FILENAMEField).getString(r);
        String PRIVATE_KEY = get(Fields.In, PRIVATE_KEY_PATHField).getString(r);
        String CERT_URL = get(Fields.In, CERT_URLField).getString(r);
        org.apache.http.client.config.RequestConfig requestConfig = org.apache.http.client.config.RequestConfig.custom().setConnectTimeout(600 * 1000).build();        
        org.apache.http.impl.client.CloseableHttpClient httpclient = org.apache.http.impl.client.HttpClientBuilder.create().setDefaultRequestConfig(requestConfig).build();
        try {

            java.io.File privateKeyFile = new java.io.File(PRIVATE_KEY);
            byte[] privateKeyBytesArray = new byte[(int) privateKeyFile.length()];
            java.io.FileInputStream privateKeyFis = new java.io.FileInputStream(privateKeyFile);
            privateKeyFis.read(privateKeyBytesArray);
            privateKeyFis.close();

            String privateKeyContent = new String(privateKeyBytesArray);

            byte[] pkcs1Key = javax.xml.bind.DatatypeConverter.parseBase64Binary(privateKeyContent.replaceAll("(-+BEGIN RSA PRIVATE KEY-+\\r?\\n|-+END RSA PRIVATE KEY-+\\r?\\n?)", ""));
            DerInputStream dis = new DerInputStream(pkcs1Key);
            DerValue[] disSequence = dis.getSequence(0);
            BigInteger modulus = disSequence[1].getBigInteger();
            BigInteger publicExp = disSequence[2].getBigInteger();
            BigInteger privateExp = disSequence[3].getBigInteger();
            BigInteger prime1 = disSequence[4].getBigInteger();
            BigInteger prime2 = disSequence[5].getBigInteger();
            BigInteger exp1 = disSequence[6].getBigInteger();
            BigInteger exp2 = disSequence[7].getBigInteger();
            BigInteger crtCoef = disSequence[8].getBigInteger();
            RSAPrivateCrtKeySpec keySpec = new RSAPrivateCrtKeySpec(modulus, publicExp, privateExp, prime1, prime2, exp1, exp2, crtCoef);
            KeyFactory keyFactory = KeyFactory.getInstance("RSA");
            PrivateKey privateKey = keyFactory.generatePrivate(keySpec);

            MessageDigest md = MessageDigest.getInstance("MD5");

            java.io.File xmlFile = new java.io.File(FILENAME);
            byte[] xmlFileBytesArray = new byte[(int) xmlFile.length()];
            java.io.FileInputStream xmlFileFis = new java.io.FileInputStream(xmlFile);
            xmlFileFis.read(xmlFileBytesArray);
            xmlFileFis.close();

            md.update(xmlFileBytesArray);
            byte[] checksum = md.digest();
            Signature privateSignature = Signature.getInstance("SHA256withRSA");
            privateSignature.initSign(privateKey);
            privateSignature.update(checksum);
            byte[] signature = privateSignature.sign();

            String fileSignature = java.util.Base64.getEncoder().encodeToString(signature);
            
            org.apache.http.client.methods.HttpPost httppost = new org.apache.http.client.methods.HttpPost(CERT_URL);
            org.apache.http.entity.mime.content.FileBody bin = new org.apache.http.entity.mime.content.FileBody(new java.io.File(FILENAME));

            org.apache.http.HttpEntity reqEntity = org.apache.http.entity.mime.MultipartEntityBuilder.create()
                    .addPart("file", bin)
                    .build();

            httppost.setHeader("signature", fileSignature);

            httppost.setEntity(reqEntity);

            System.out.println("executing request " + httppost.getRequestLine());
            org.apache.http.client.methods.CloseableHttpResponse response = httpclient.execute(httppost);
            try {
                Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());
                get(Fields.Out, RESPONSEField).setValue(outputRow, response.getStatusLine());
                putRow(data.outputRowMeta, outputRow);
            } finally {
                response.close();
            }
        } catch (Exception e) {
            e.getMessage();
        } finally {
            httpclient.close();
        }


        return true;
    }

   



  





     
