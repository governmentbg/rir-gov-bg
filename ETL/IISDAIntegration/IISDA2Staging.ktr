        <class_source>import java.utils.*;
import java.lang.*;
import java.net.*;
import org.w3c.dom.Document;
import org.xml.sax.InputSource;
import javax.xml.parsers.DocumentBuilder;
import javax.xml.parsers.DocumentBuilderFactory;
import java.io.StringReader;
import java.io.InputStream;
import java.io.StringReader;
import java.nio.charset.StandardCharsets;
import java.io.ByteArrayInputStream;

String XMLField;
String XMLValidatedField;	
					
									

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException 
{ 
   
          XMLField = getParameter("XMLField");
          XMLValidatedField = getParameter("XMLValidatedField");
           
     
        Object[] r = getRow();

     if (r == null) {
      setOutputDone();
      return false;
    }



       String XML = get(Fields.In, XMLField).getString(r);
      Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());
try{
            DocumentBuilderFactory dbF = DocumentBuilderFactory.newInstance();
            DocumentBuilder builder = dbF.newDocumentBuilder();
            InputSource inputSource = new InputSource(new StringReader(XML));
// the "parse" method also validates XML, will throw an exception if misformatted
            Document document = builder.parse(inputSource);

           get(Fields.Out, XMLValidatedField).setValue(outputRow, "yes");
                    putRow(data.outputRowMeta, outputRow);
}   catch (Exception e) {
       
           get(Fields.Out, XMLValidatedField).setValue(outputRow, e.getMessage());
                    putRow(data.outputRowMeta, outputRow);
}
        
        return true;
}




  





     
